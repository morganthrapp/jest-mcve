{
	"compilerOptions": {
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"strictNullChecks": true,
		"target": "es5",
		"forceConsistentCasingInFileNames": true,
		"module": "commonjs",
		"jsx": "react",
		"declaration": true,
		"sourceMap": true,
		"experimentalDecorators": true,
		"skipLibCheck": true,
		"typeRoots": [
			"./node_modules/@types"
		],
		"types": [
			"es6-promise",
			"webpack-env"
		],
		"lib": [
			"es5",
			"dom",
			"es2015.collection"
		]
	}
}
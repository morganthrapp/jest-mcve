{
	"devDependencies": {
		"@types/jest": "^22.2.3",
		"jest": "^22.4.3",
		"ts-jest": "^22.4.2",
		"typescript": "^2.8.1"
	},
	"scripts": {
		"test": "jest"
	},
	"jest": {
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "tests?(?:\\|/)(?:(?!helpers?).*(?:\\|/))*.*[Tt]ests?.*\\.tsx?",
		"moduleNameMapper": {
			"TestModule": "<rootDir>/typings.d.ts"
		},
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"d.ts"
		]
	}
}